{
  "name": "@adexchange/aeg-sdk",
  "version": "0.0.3",
  "description": "SDK for CAMP 2.0",
  "main": "lib/index.js",
  "author": "Ad Exchange Group",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-sdk.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive test/",
    "test:coverage": "istanbul cover _mocha -- --recursive test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\"",
    "build": "npm run jshint && mocha --recursive test/unit/ -R xunit-file",
    "postversion": "git push && git push --tags && npm publish ."
  },
  "devDependencies": {
    "should": "^5.2.0",
    "mocha": "^2.2.5",
    "onchange": "^1.1.0",
    "parallelshell": "^2.0.0",
    "istanbul": "^0.3.17",
    "xunit-file": "0.0.6"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "0.0.23",
    "config": "^1.16.0",
    "express-stormpath": "^2.2.0",
    "q": "^1.4.1",
    "request": "^2.65.0",
    "swagger-js-codegen": "^1.1.5",
    "yargs": "^3.29.0"
  },
  "engine": "node >= 4.1.0"
}
