{
  "name": "@adexchange/aeg-sdk",
  "version": "0.0.49",
  "description": "SDK for CAMP 2.0",
  "main": "lib/index.js",
  "author": "Ad Exchange Group",
  "repository": {
    "type": "git",
    "url": "git://git@bitbucket.org:AdExchangeGrp/aeg-sdk.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register test/",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers js:babel-core/register test/",
    "jshint": "jshint .",
    "jshint:watch": "npm run jshint && onchange \"**/*.js\" \".jshint*\" -- npm run jshint",
    "dev": "parallelshell \"npm run jshint:watch\"",
    "build": "npm run jshint && mocha --recursive --compilers js:babel-core/register test/unit/ -R xunit-file",
    "compile": "babel -d ./lib/ ./src/",
    "prepublish": "npm run compile",
    "postversion": "git push && git push --tags && npm publish ."
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "onchange": "^1.1.0",
    "parallelshell": "^2.0.0",
    "should": "^5.2.0",
    "xunit-file": "0.0.6"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "0.0.24",
    "async": "^1.5.0",
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "config": "^1.16.0",
    "express-stormpath": "^2.2.0",
    "njwt": "^0.2.3",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "request": "^2.65.0",
    "stormpath": "^0.13.5",
    "swagger-js-codegen": "^1.1.5",
    "underscore": "^1.8.3",
    "yargs": "^3.29.0"
  },
  "engine": "node >= 4.1.0"
}
